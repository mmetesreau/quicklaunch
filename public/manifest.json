{
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "version": "3",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "tabs",
        "notifications",
        "bookmarks"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "commandPalette.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F",
                "linux": "Ctrl+Shift+F"
            }
        }
    }
}